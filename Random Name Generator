<html>
<head>
<script type = "text/javascript">
  
    

   function randomName(maxValue)
    {
        return Math.floor(Math.random() * maxValue)

    }

    var firstName = [];
    firstName.push('Jack');
    firstName.push('John')
    firstName.push('Alex')
    firstName.push('Marty')
    firstName.push('Andrew')
    firstName.push('Alexis')
    firstName.push('Sarah')
    firstName.push('Bri')
    firstName.push('Natasha')
    firstName.push('Christy')


    var lastName = [];
    lastName.push('Wellman');
    lastName.push('Weasley')
    lastName.push('Golden')
    lastName.push('Pruitt')
    lastName.push('Goodman')
    lastName.push('Snuggles')
    lastName.push('Pitfall')
    lastName.push('Debt')
    lastName.push('Backerton')
    lastName.push('Hooverson')


    var firstNameIndex = randomName(firstName.length);
    var lastNameIndex = randomName(lastName.length);

    console.log(firstName[firstNameIndex]);
    console.log(lastName[lastNameIndex]);

</script>
</head>


<body>

<h1>
    Random Name Generator!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
</h1>





</body>









</script>
